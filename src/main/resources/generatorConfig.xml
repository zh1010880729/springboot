            <columnOverride column="is_activity" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="saas_supply_activity" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="is_print_price" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="user_type" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="credit_level" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="member_level" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="rounding_type" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
            <columnOverride column="enabled" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
