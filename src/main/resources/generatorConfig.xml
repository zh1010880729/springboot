        <table tableName="customer_member_level" domainObjectName="CustomerMemberLevel" enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <!--类型重写-->
            <columnOverride column="member_level" jdbcType="TINYINT" javaType="java.lang.Integer"></columnOverride>
        </table>
    </context>
</generatorConfiguration>